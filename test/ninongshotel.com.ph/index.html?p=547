<script> 

 
(function($) {


	function checkOffset() {
    if($('#right-sidebar').offset().top + $('#right-sidebar').height() 
                                           >= $('#wrapper-footer').offset().top - 10)
        $('#right-sidebar').css('position', 'absolute');
    if($(document).scrollTop() + window.innerHeight < $('#wrapper-footer').offset().top)
        $('#right-sidebar').css('position', 'fixed'); // restore when you scroll up
	}
 
    // fire when above 1203
    if (document.documentElement.clientWidth > 1203) {
     
// fixed un 

	// sticky navbar
	$(window).scroll(function() { 
		 checkOffset();
	});

	// sticky navbar
	$(document).ready(function() {
		 checkOffset();
	});
     
    }
    // fire when below 1203
    else {
      console.log('Smaller!');
    }  
 


    	window.addEventListener("resize", function(){
    // fire when above 1203
    if (document.documentElement.clientWidth > 1203) {
     
// fixed un 

	// sticky navbar
	$(window).scroll(function() { 
		 checkOffset();
	});
 
    }
    // fire when below 1203
    else {
      console.log('Smaller!');
    }
}, true);
 



})(jQuery);
</script>	
